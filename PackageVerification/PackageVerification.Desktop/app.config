  <appSettings>
    <add key="TestDBServerAddress" value="{server name}" />
    <add key="TestDBServerUserName" value="{username}" />
    <add key="TestDBServerPassword" value="{password}" />
    <add key="TestDBPassword" value="{password}" />

    <add key="AzureInstallConnectionString" value="Server={server};Database={database};User ID={user};Password={password};Trusted_Connection=False;Encrypt=True;Connection Timeout=120;" />
    <add key="TestInstallDBName" value="{db name}" />
    <add key="AzureRestoreConnectionString" value="Server={server};Database={database};User ID={user};Password={password};Trusted_Connection=False;Encrypt=True;Connection Timeout=120;" />
    <add key="TestRestoreDBName" value="{db name}" />
    <add key="FileToProcess" value="C:\SQLAzureMW\BCPData\Northwind2.sql" />
    <add key="OutputResultsFile" value="C:\SQLAzureMW\BCPData\Output.sql" />
    <add key="NoConsoleLogging" value="false" />
    <!-- NOTE BCP requires that you have the SQL Azure server name the end of the user name ie. UserSA@jy8kadhrma -->
    <add key="TargetConnectNTAuth" value="false" />
    <add key="TargetServerName" value="SERVER.database.windows.net" />
    <add key="TargetUserName" value="UserName@SERVER" />
    <add key="TargetPassword" value="" />
    <add key="TargetDatabase" value="" />
    <add key="TargetServerType" value="ServerType_AzureSQLDatabaseV12" />
    <!-- ServerType_AzureSQLDatabase = "Azure SQL Database"
                                                                             ServerType_AzureSQLDatabaseV12 = "Azure SQL Database Latest Preview V12"
                                                                             ServerType_SQLServer = "SQL Server" -->
    <add key="ConnectionStringTrusted" value="server={0};database={1};Trusted_Connection=yes;Encrypt=true;TrustServerCertificate=True;Connection Timeout=15" />
    <add key="ConnectionString" value="server={0};database={1};uid={2};pwd={3};Encrypt=true;TrustServerCertificate=True;Connection Timeout=15" />
    <add key="DBCollation" value="" />
    <!-- Leave blank for server default otherwise specify (i.e. Latin1_General_100_CI_AI) -->
    <add key="EndOfLife_Year" value="2015" />
    <add key="EndOfLife_Month" value="9" />
    <add key="EndOfLife_Day" value="1" />
    <add key="EndOfLife_Link" value="http://msdn.microsoft.com/library/azure/dn741330.aspx" />
    <add key="ServiceTiersPerformanceLevels_Link" value="http://msdn.microsoft.com/en-us/library/azure/dn741336" />
    <add key="RetryCount" value="5" />
    <add key="RetryInitialDelay" value="00:00:00.5" />
    <add key="RetryMinimunDelay" value="00:00:00.2" />
    <add key="RetryMaximunDelay" value="00:00:05" />
    <add key="BCPExe" value="bcp.exe" />
    <!-- BCP.EXE - You can specify directory if needed -->
    <add key="BCPFileDir" value="c:\SQLAzureMW\BCPData" />
    <!-- Input / Output file location for BCP table data.  If blank, then will use temp directory -->
    <add key="DelOldBCPFiles" value="true" />
    <!-- Deletes old BCP files if exists, otherwise, it will add a unique numeric ext -->
    <add key="BCPFileExt" value="dat" />
    <!-- BCP output file extension -->
    <add key="DeleteBCPOutputFiles" value="false" />
    <!-- If true, SQLAzureMW will delete the temporary BCP output files after successfully uploading to target server -->
    <add key="TimeToSleepOnConnectionReset" value="2500" />
    <!-- Number of miliseconds to sleep when resetting connection -->
    <add key="TimeBetweenChunks" value="15000" />
    <!-- Number of miliseconds to sleep between BCP chuncks -->
    <add key="ChunkSize" value="100000000" />
    <!-- Number of records to process per BCP iteration (Uploading to Azure) -->
    <add key="BCPArgsIn" value="{0} in {1} -E -n -C RAW -b 1000 -a 4096" />
    <!-- BCP in command parameters -->
    <add key="BCPArgsOut" value="&quot;{0}&quot; out {1} -E -n -C RAW" />
    <!-- BCP out command parameters -->
    <add key="NotSupportedByAzureSQLDb" value="NotSupportedByAzureSQLDb.Config" />
    <add key="NotSupportedByAzureSQLDbV12" value="NotSupportedByAzureSQLDbV12.Config" />
    <add key="NumberOfBCPThreads" value="8" />
    <add key="BCPSQLAzureErrorCodesRetry" value="SQLState\s=\s(-2|64|233|08001|08S01|10053|10054|10060|37000|40001|40143|40174|40197|40501|40544|40549|40550|40551|40552|40553|40613|40615),\sNativeError\s=\s[0-9]+" />
    <!-- Connection-Loss Errors search -->
    <!-- General Options -->
    <add key="ScriptDefaults" value="true" />
    <add key="ScriptHeaders" value="false" />
    <add key="ScriptExtendedProperties" value="true" />
    <add key="IncludeIfNotExists" value="false" />
    <add key="ScriptDropCreate" value="SOSCreate" />
    <!-- SOSCreate = "Script CREATE",
                                                                            SOSDrop = "Script DROP",
                                                                            SOSDropCreate = "Script DROP and CREATE" -->
    <!-- Table / View Options -->
    <add key="ScriptTableAndOrData" value="ScriptOptionsTableSchemaData" />
    <!-- ScriptOptionsTableSchema = "Table Schema Only",
                                                                              ScriptOptionsTableSchemaData = "Table Schema with Data",
                                                                              ScriptOptionsTableData = "Data Only" -->
    <add key="ScriptCheckConstraints" value="true" />
    <add key="ScriptCollation" value="true" />
    <add key="ScriptForeignKeys" value="true" />
    <add key="ScriptIndexes" value="true" />
    <add key="ScriptPrimaryKeys" value="true" />
    <add key="ScriptUniqueKeys" value="true" />
    <add key="ScriptTableTriggers" value="true" />
    <!-- Check For Options-->
    <add key="ActiveDirectorySP" value="false" />
    <add key="BackupandRestoreTable" value="false" />
    <add key="ChangeDataCapture" value="false" />
    <add key="CompatibilityChecks" value="ScriptOptionsOverride" />
    <!-- ScriptOptionsOverride = "Override: Perform All Compatibility Checks",
                                                                          ScriptOptionsUseDefault = "Use Default Settings",
                                                                          ScriptOptionsOverrideNot = "Override: Do Not Perform Compatibility Checks" -->
    <add key="CursorSPs" value="false" />
    <add key="DatabaseEngineSP" value="false" />
    <add key="DatabaseMailSP" value="false" />
    <add key="DatabaseMaintenancePlan" value="false" />
    <add key="DataControl" value="false" />
    <add key="DistributedQueriesSP" value="false" />
    <add key="FullTextSearchSP" value="false" />
    <add key="GeneralExtendedSPs" value="false" />
    <add key="GeneralTSQL" value="true" />
    <add key="IntegrationServicesTable" value="false" />
    <add key="LogShipping" value="false" />
    <add key="MetadataFunction" value="false" />
    <add key="OLEAutomationSP" value="false" />
    <add key="OLEDBTable" value="false" />
    <add key="ProfilerSP" value="false" />
    <add key="ReplicationSP" value="false" />
    <add key="ReplicationTable" value="false" />
    <add key="RowsetFunction" value="false" />
    <add key="SecurityFunction" value="false" />
    <add key="SecuritySP" value="false" />
    <add key="SQLMailSP" value="false" />
    <add key="SQLServerAgentSP" value="false" />
    <add key="SQLServerAgentTable" value="false" />
    <add key="SystemCatalogView" value="false" />
    <add key="SystemFunction" value="false" />
    <add key="SystemStatisticalFunction" value="false" />
    <add key="Unclassified" value="false" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
